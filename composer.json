{
    "name": "osjava/laravel-tools",
    "description": "This is some tools for applications develop on laravel",
    "keywords": ["laravel", "apidoc", "asciidoc"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "samboy",
            "email": "samboy@osjava.net"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/finder": "^5.1",
        "laravel/framework": "^8.0",
        "ext-json": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "QFrame\\": "src/"
        },
        "files": [
            "src/Support/helpers.php",
            "src/Support/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QFrame\\Providers\\ToolsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^6.7"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    }
}
